# Google Cloud project ID and region for deployment
PROJECT_ID=your-project-id
REGION=us-central1

# Artifact Registry repository where the container image will be pushed
REPOSITORY=vertex-yolo-easyocr
IMAGE_NAME=vertex-yolo-easyocr
IMAGE_TAG=latest
IMAGE_URI=${REGION}-docker.pkg.dev/${PROJECT_ID}/${REPOSITORY}/${IMAGE_NAME}:${IMAGE_TAG}

# Vertex AI model and endpoint names
MODEL_NAME=vertex-yolo-easyocr
ENDPOINT_NAME=vertex-yolo-easyocr

# Service account used for deployment (must have necessary Vertex AI and Artifact Registry roles)
SERVICE_ACCOUNT=service-account@${PROJECT_ID}.iam.gserviceaccount.com

# Machine type used for the deployed endpoint
MACHINE_TYPE=n1-standard-4
